module Servomotor_Test(
	input Clock,
	input [2:0] Switches,
	output servomotor
);

reg [11:0] ciclo=0;

always @(posedge Clock)begin 

	if(Switches[0]==1'b1)ciclo=12'd199;
	else if(Switches[1]==1'b1)ciclo=12'd299;
	else if(Switches[2]==1'b1)ciclo=12'd399;

end


Servomotor xD(Clock, 16'd250, ciclo, servomotor);



endmodule
